{
  "name": "remix-template-remix",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:css && remix build",
    "build:css": "postcss -o app/tailwind.css ./styles/tailwind.css",
    "dev:css": "postcss -o app/tailwind.css ./styles/tailwind.css --watch",
    "download-schema": "apollo service:download --endpoint=http://localhost:1337/graphql types/graphql-schema.json",
    "generate-types": "apollo codegen:generate --target=typescript --tagName=gql",
    "dev": "concurrently \"yarn dev:css\" \"remix dev\"",
    "start": "remix-serve build"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.6.6",
    "@date-io/date-fns": "^2.14.0",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.2",
    "@mui/material": "^5.8.2",
    "@mui/x-date-pickers": "^5.0.0-alpha.5",
    "@prisma/client": "^3.14.0",
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "@types/tailwindcss": "^3.0.10",
    "date-fns": "^2.28.0",
    "framer-motion": "^6.3.6",
    "prisma": "^3.14.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-lazy-load-image-component": "^1.5.4",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "apollo": "^2.34.0",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "eslint": "^8.16.0",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2"
  },
  "engines": {
    "node": ">=14"
  }
}
