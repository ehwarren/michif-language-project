{
  "name": "remix-template-remix",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:css && remix build",
    "build:css": "postcss -o app/tailwind.css ./styles/tailwind.css",
    "dev:css": "postcss -o app/tailwind.css ./styles/tailwind.css --watch",
    "download-schema": "apollo service:download --endpoint=http://localhost:1337/graphql types/graphql-schema.json",
    "generate-types": "apollo codegen:generate --target=typescript --tagName=gql",
    "dev": "concurrently \"yarn dev:css\" \"remix dev\"",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@prisma/client": "^3.12.0",
    "@remix-run/node": "^1.3.4",
    "@remix-run/react": "^1.3.4",
    "@remix-run/serve": "^1.3.4",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "framer-motion": "^6.2.10",
    "prisma": "^3.12.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-lazy-load-image-component": "^1.5.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.4",
    "@remix-run/eslint-config": "^1.3.4",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "apollo": "^2.33.10",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.1.0",
    "eslint": "^8.11.0",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
